{
  "manifest_version": 3,
  "name": "AutoMeet",
  "version": "1.0",
  "description": "AI-powered meeting summarizer Chrome extension",
  "permissions": ["activeTab", "storage"],
  "host_permissions": [
      "http://127.0.0.1:5000/*" 
  ],
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "icon16.png",
          "48": "icon48.png",
          "128": "icon128.png"
      }
  },
  "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
  },
  "background": {
      "service_worker": "background.js"
  }
}
